			function reset() {
				bfPlayer = null;
				afPlayer = null;
				changeText = "";
				changeCount = 0;
				bfPositions = "";
				afPositions = "";
				positionChange = false;
				$("#ApplicationText").val("");
				$("#CA").css("color", "black").css("font-weight", "normal").css("font-size", "16");
				$("#Position").css("color", "black").css("font-weight", "normal").css("font-size", "16");
				$("#Consistency").removeClass();
				$("#Consistency").removeClass();
				$("#Dirtiness").removeClass();
				$("#ImportantMatches").removeClass();
				$("#InjuryProness").removeClass();
				$("#Versatility").removeClass();
				$("#Adaptability").removeClass();
				$("#Ambition").removeClass();
				$("#Loyalty").removeClass();
				$("#Pressure").removeClass();
				$("#Professional").removeClass();
				$("#Sportsmanship").removeClass();
				$("#Temperament").removeClass();
				$("#Controversy").removeClass();
				$("#Aggression").removeClass();
				$("#Anticipation").removeClass();
				$("#Bravery").removeClass();
				$("#Composure").removeClass();
				$("#Concentration").removeClass();
				$("#Decisions").removeClass();
				$("#Determination").removeClass();
				$("#Flair").removeClass();
				$("#Leadership").removeClass();
				$("#OffTheBall").removeClass();
				$("#Positioning").removeClass();
				$("#Teamwork").removeClass();
				$("#Vision").removeClass();
				$("#Workrate").removeClass();
				$("#Acceleration").removeClass();
				$("#Agility").removeClass();
				$("#Balance").removeClass();
				$("#Jumping").removeClass();
				$("#LeftFoot").removeClass();
				$("#NaturalFitness").removeClass();
				$("#Pace").removeClass();
				$("#RightFoot").removeClass();
				$("#Stamina").removeClass();
				$("#Strength").removeClass();
				$("#Corners").removeClass();
				$("#Crossing").removeClass();
				$("#Dribbling").removeClass();
				$("#Finishing").removeClass();
				$("#FirstTouch").removeClass();
				$("#Freekicks").removeClass();
				$("#Heading").removeClass();
				$("#LongShots").removeClass();
				$("#Longthrows").removeClass();
				$("#Marking").removeClass();
				$("#Passing").removeClass();
